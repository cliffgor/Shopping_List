{
  "name": "MERN_SHOPPING_LIST",
  "version": "1.0.0",
  "description": "this is a shopping list app built with the MERN stack",
  "main": "server.js",
  "scripts": {
    "client-install": "yarn install --prefix client",
    "start": "node server.js",
    "client": "yarn --cwd client/ start",
    "server": "nodemon server.js",
    "dev": "concurrently \"yarn run server\" \"yarn run client\""
  },
  "author": {
    "name": "Cliff Gor"
  },
  "license": "MIT",
  "dependencies": {
    "axios": "^0.21.1",
    "body-parser": "^1.19.0",
    "concurrently": "^5.0.0",
    "express": "^4.17.1",
    "mongoose": "^5.7.9",
    "react-scripts": "^3.3.0"
  },
  "devDependencies": {
    "nodemon": "^1.19.4"
  }
}
